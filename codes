local ReplicatedStorage = game:GetService("ReplicatedStorage")
local discoverEvent = ReplicatedStorage:WaitForChild("events"):WaitForChild("discoverlocation")

local zonesFolder = workspace:WaitForChild("zones"):WaitForChild("player")

for _, zone in pairs(zonesFolder:GetChildren()) do
    if zone:FindFirstChild("discover") then
        discoverEvent:FireServer(zone.Name)
    end
end

local codes = {
    "NorthernExpedition",
    "CAVERN",
    "LEGOMY",
    "SorryReward",
    "THEKRAKEN",
    "CARBON",
    "ATLANTEANSTORM",
    "SORRYGUYS",
    "GOLDENTIDE",
    "FISCHMASDAY",
    "NewYear"
}


local runcode = game:GetService("ReplicatedStorage").events.runcode

for _, code in ipairs(codes) do
    pcall(function()
        runcode:FireServer(code)
    end)
    task.wait(0.1)
end
